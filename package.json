{
  "name": "wheeled.ai",
  "version": "0.1.0",
  "private": true,
  "author": "Nick Mezacapa",
  "email": "nmm.developer@gmail.com",
  "description": "An innovative AI-powered vehicle inspection system designed to automate and simplify the inspection process for rideshare drivers, ensuring safety and efficiency.",
  "browserslist": "defaults, not ie <= 11",
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "build": "next build",
    "db:push": "prisma db push",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "fix:lint": "eslint --fix .",
    "format": "prettier --write .",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest --detectOpenHandles",
    "test:components": "jest --detectOpenHandles --testPathPattern=__tests__/unit/components",
    "test:file": "jest --detectOpenHandles --testPathPattern=__tests__/unit",
    "test:hooks": "jest --detectOpenHandles --testPathPattern=__tests__/unit/hooks"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "@t3-oss/env-nextjs": "^0.6.0",
    "@tanstack/react-query": "^4.32.6",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@trpc/client": "^10.37.1",
    "@trpc/next": "^10.37.1",
    "@trpc/react-query": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "clsx": "^2.0.0",
    "focus-visible": "^5.2.0",
    "framer-motion": "^10.16.4",
    "next": "^13.4.19",
    "next-seo": "^6.1.0",
    "next-themes": "^0.2.1",
    "postcss-focus-visible": "^9.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^1.13.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/eslint": "^8.44.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.7.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.1",
    "prisma": "^5.1.1",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.20.2"
  },
  "packageManager": "yarn@1.22.17",
  "resolutions": {
    "webpack": "^5"
  }
}
